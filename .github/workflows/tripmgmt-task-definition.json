{
  "family": "ECS-EC2",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "EC2"
  ],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::891612582051:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::891612582051:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "cpu": 512,
      "environment": [
        {
          "name": "SPRING_DATASOURCE_URL",
          "value": "jdbc:postgresql://comfortable-shark-database.cc3w0qkyopxz.us-east-1.rds.amazonaws.com:5432/postgres"
        },
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "prod"
        },
        {
          "name": "SPRING_DATASOURCE_USERNAME",
          "value": "postgres"
        },
        {
          "name": "SPRING_LIQUIBASE_USER",
          "value": "postgres"
        },
        {
          "name": "SPRING_LIQUIBASE_URL",
          "value": "jdbc:postgresql://comfortable-shark-database.cc3w0qkyopxz.us-east-1.rds.amazonaws.com:5432/postgres"
        }
      ],
      "secrets": [
        {
          "name": "SPRING_DATASOURCE_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:891612582051:secret:rds!db-975956db-521a-45bf-83db-a15023873da7-gyCn74:password::"
        },
        {
          "name": "SPRING_LIQUIBASE_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:891612582051:secret:rds!db-975956db-521a-45bf-83db-a15023873da7-gyCn74:password::"
        }
      ],
      "environmentFiles": [],
      "essential": true,
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:8080/ || exit 1"
        ],
        "interval": 10,
        "retries": 3,
        "timeout": 5
      },
      "image": "891612582051.dkr.ecr.us-east-1.amazonaws.com/comfortable-shark/tripmgmt@sha256:2793f6c942b33e41b064c7e7943f135113221df4ef9b2e23de9d30a4d9802009",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/tripmgmt-task",
          "awslogs-create-group": "true",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "mountPoints": [],
      "name": "tripmgmt",
      "portMappings": [
        {
          "appProtocol": "http",
          "containerPort": 8080,
          "hostPort": 8080,
          "name": "tripmgmt-8080-tcp",
          "protocol": "tcp"
        }
      ],
      "systemControls": [],
      "ulimits": [],
      "volumesFrom": []
    }
  ]
}